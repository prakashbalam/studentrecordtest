     200:
       body:
         application/json:
           example:
            { "name" : "prakash",
             "id" : "789123",
             "year" : "2018",
             "course" : "MCA"
            } 
 delete:
  description:
  responses:
    200:
      body:
        application/json:
          example: 
           { "name" : "Ashok",
           "id" : "123456",
           "year" : "2014",
           "course" : "B.Tech"
           }