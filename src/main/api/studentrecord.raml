/student:
 get:
   responses:
     200:
       body:
         application/json:
           example: !
             { "root":
               { "name": "Ashok",
               "id": "123456",
               "year": "2014",
               "course": "B.Tech"
               },
                 "name": "sunny",
                  "id": "456789",
                  "year": "2015",
                  "course": "M.Tech"
             }
               
 put:
  description: update an existing record in the list 
